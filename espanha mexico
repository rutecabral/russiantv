 source2 = requests.get(embed2, headers=headers)
 embed3 = re.findall('script type="text\/javascript" src="(.*?)"', source2.text)[0]
 headers2 = {'User-Agent': 'Mozilla/5.0 (X11; Linux i686; rv:42.0) Gecko/20100101 Firefox/42.0 Iceweasel/42.0', 'Referer': 'http://verdirectotv.com/'}
 source3= requests.get(embed3, headers = headers2)
 embed4 = re.findall('iframe src="(.*?)"', source3.text)[0]
 source4= requests.get(embed4, headers = headers2)
 m3u8_encoded = re.findall('input type="hidden.*?\n.*?\n.*?input type="hidden".*?value="(.*?)"', source4.text)[0]
 m3u8= base64.b64decode(m3u8_encoded)


 return m3u8 + '|Referer=http://www.playerhd2.pw/jwplayer6/jwplayer.flash.swf&User-Agent=Mozilla/5.0 (X11; Linux i686; rv:42.0) Gecko/20100101 Firefox/42.0 Iceweasel/42.0'
